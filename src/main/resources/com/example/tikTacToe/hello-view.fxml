<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" spacing="20.0" stylesheets="@css/styles.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tikTacToe.controller.HelloController">

    <BorderPane fx:id="pane2" maxHeight="-Infinity" maxWidth="-Infinity" stylesheets="css/styles.css">
        <right>
            <Label text="Label" BorderPane.alignment="CENTER">
            <padding>
               <Insets right="100.0" />
            </padding></Label>
        </right>


        <center>
          <GridPane alignment="CENTER" gridLinesVisible="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
             <columnConstraints>
               <ColumnConstraints minWidth="10.0" prefWidth="50.0" />
               <ColumnConstraints minWidth="10.0" prefWidth="50.0" />
               <ColumnConstraints minWidth="10.0" prefWidth="50.0" />
           </columnConstraints>
             <rowConstraints>
               <RowConstraints minHeight="10.0" prefHeight="50.0" />
               <RowConstraints minHeight="10.0" prefHeight="50.0" />
               <RowConstraints minHeight="10.0" prefHeight="50.0" />
           </rowConstraints>
            <children>
               <ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
            </children>
         </GridPane>
      </center>
      <bottom>
         <HBox prefHeight="50.0" BorderPane.alignment="CENTER">
            <children>
               <Button alignment="CENTER_RIGHT" mnemonicParsing="false" text="Button" />
               <Button mnemonicParsing="false" text="Button" />
            </children>
         </HBox>
      </bottom>
      <left>
         <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
               <Label text="Label" />
               <Label alignment="BOTTOM_CENTER" text="Label" />
            </children>
         </VBox>
      </left>
      <top>
         <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
               <Label text="Label" />
            </children>
         </HBox>
      </top>
    </BorderPane>
</HBox>
